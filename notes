{% block content %}
<div class="dashboard-header"> <h1>Welcome back!</h1> <p class="text-muted">Here's what's happening with your contracts today.</p> </div> <div class="stats-grid"> <div class="stat-card contracts"> <div class="stat-icon"> <i class="fas fa-file-contract"></i> </div> <div class="stat-content"> <h3>Active Contracts</h3> <div class="stat-value">{{ total_contracts }}</div> <div class="stat-action"> <a href="{% url 'contract_list' %}" class="btn-link"> View all contracts <i class="fas fa-arrow-right"></i> </a> </div> </div> </div>

