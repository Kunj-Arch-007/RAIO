.menu-toggle {
display: block;
}

.sidebar {
    width: var(--sidebar-collapsed-width);
    transform: translateX(-100%);
}

.sidebar .logo span,
.sidebar .menu-item span,
.sidebar .menu-header {
    display: none;
}

.main-content {
    margin-left: 0;
}

body.sidebar-collapsed .sidebar {
    transform: translateX(0);
    width: var(--sidebar-width);
}

body.sidebar-collapsed .sidebar .logo span,
body.sidebar-collapsed .sidebar .menu-item span,
body.sidebar-collapsed .sidebar .menu-header {
    display: inline;
}

