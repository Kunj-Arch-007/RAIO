import React, { useState } from 'react';
import { 
  LayoutDashboard, 
  Users, 
  FileContract, 
  BanknoteIcon, 
  FileInvoice,
  Handshake,
  Menu,
  User,
  LogOut
} from 'lucide-react';

function App() {
  const [isSidebarCollapsed, setSidebarCollapsed] = useState(false);
  const [isDropdownOpen, setDropdownOpen] = useState(false);

  const toggleSidebar = () => setSidebarCollapsed(!isSidebarCollapsed);
  const toggleDropdown = () => setDropdownOpen(!isDropdownOpen);

  return (
    <div className="min-h-screen bg-gray-50">
      {/* Sidebar */}
      <aside className={`fixed inset-y-0 left-0 bg-slate-900 text-white transition-all duration-300 ease-in-out z-30
        ${isSidebarCollapsed ? 'w-20 -translate-x-full lg:translate-x-0' : 'w-72'}`}>
        {/* Logo */}
        <div className="h-16 flex items-center px-6 border-b border-slate-800">
          <a href="/" className="flex items-center gap-3 text-xl font-semibold">
            <LayoutDashboard className="h-6 w-6" />
            {!isSidebarCollapsed && <span>RAIO</span>}
          </a>
        </div>

        {/* Navigation */}
        <nav className="py-6">
          <a href="/" className="flex items-center gap-3 px-6 py-3 text-slate-300 hover:bg-slate-800 hover:text-white transition-colors">
            <LayoutDashboard className="h-5 w-5" />
            {!isSidebarCollapsed && <span>Dashboard</span>}
          </a>
          <a href="/clients" className="flex items-center gap-3 px-6 py-3 text-slate-300 hover:bg-slate-800 hover:text-white transition-colors">
            <Users className="h-5 w-5" />
            {!isSidebarCollapsed && <span>Healthcare Clients</span>}
          </a>
          <a href="/contracts" className="flex items-center gap-3 px-6 py-3 text-slate-300 hover:bg-slate-800 hover:text-white transition-colors">
            <FileContract className="h-5 w-5" />
            {!isSidebarCollapsed && <span>Service Agreements</span>}
          </a>

          {!isSidebarCollapsed && (
            <div className="mt-6 px-6">
              <h3 className="text-xs font-semibold text-slate-400 uppercase tracking-wider">Financials</h3>
            </div>
          )}
          
          <a href="/expenses" className="flex items-center gap-3 px-6 py-3 text-slate-300 hover:bg-slate-800 hover:text-white transition-colors">
            <BanknoteIcon className="h-5 w-5" />
            {!isSidebarCollapsed && <span>Operational Costs</span>}
          </a>
          <a href="/invoices" className="flex items-center gap-3 px-6 py-3 text-slate-300 hover:bg-slate-800 hover:text-white transition-colors">
